{
  "mcpServers": {
    "gohighlevel": {
      "command": "python",
      "args": ["-m", "gohighlevel_mcp.server"],
      "cwd": "/path/to/gohighlevel-mcp",
      "env": {
        "GHL_API_KEY": "your_api_key_here",
        "GHL_LOCATION_ID": "your_location_id_here",
        "GHL_API_VERSION": "v1"
      }
    }
  }
}